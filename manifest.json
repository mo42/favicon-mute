{
  "manifest_version": 3,
  "name": "Favicon Mute",
  "version": "1.0",
  "description": "Browser extension that prevents web apps from changing the favicon to display notifications",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "32": "icon_32.png",
    "64": "icon_64.png"
  }
}
