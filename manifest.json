{
  "manifest_version": 2,
  "name": "Favicon Mute",
  "version": "1.1",
  "description": "Browser extension that prevents web apps from changing the favicon to display notifications",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png"
  }
}
